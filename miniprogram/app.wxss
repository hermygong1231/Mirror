/* 全局样式 */
page {
  background: #f9fafb;
  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif;
  color: #111827;
  font-size: 28rpx;
  line-height: 1.5;
}

button {
  padding: 0;
  margin: 0;
  line-height: inherit;
}

button::after {
  border: 0;
}
